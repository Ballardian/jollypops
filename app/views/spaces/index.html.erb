<div class="container">
  <h1>All spaces</h1>
  <%= link_to "Add Space", new_space_path, :class => "btn-medium" %>
  <div class="row">
        <% @spaces.each do |space| %>
          <div class="col-xs-12 col-sm-6 col-md-4">
          <%= link_to space_path(space) do %>

            <div class="card-spaces" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.2)), url( <%= image_path space.photo %> );">
              <div class="card-category"></div>
              <div class="card-description">
                <h2><%= space.name %></h2>
              </div>
              <img class="card-user">
            </div>
          <% end %>
    </div>
      <% end %>
  </div>
</div>


<div class="container">
  <!-- Normal Demo-->
  <div class="column">

    <!-- Post-->
    <div class="post-module">
      <!-- Thumbnail-->
      <div class="thumbnail">
        <div class="date">
          <div class="day">27</div>
          <div class="month">Mar</div>
        </div><img src="https://s3-us-west-2.amazonaws.com/s.cdpn.io/169963/photo-1429043794791-eb8f26f44081.jpeg"/>
      </div>
      <!-- Post Content-->
      <div class="post-content">
        <div class="category">Click for more info</div>
        <h1 class="title">City Lights in New York</h1>
