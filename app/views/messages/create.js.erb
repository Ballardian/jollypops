function addMessage(messageHTML) {
  const notifications = document.getElementById('notifications');
  notifications.insertAdjacentHTML('beforeend', messageHTML);
}

<% unless @message.errors.any? %>
  addMessage('<%= j render "messages/show", message: @message %>');
  newMessageForm = document.getElementById('message_content');
  newMessageForm.value = '';
<% end %>
